{
  "name": "puppies-react",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "node-sass": "^4.5.0",
    "react-scripts": "0.9.0"
  },
  "dependencies": {
    "bulma": "^0.3.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "scripts": {
    "build-css": "node-sass src/ --include-path node_modules/bulma/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ --include-path node_modules/bulma/ -o src/ --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
